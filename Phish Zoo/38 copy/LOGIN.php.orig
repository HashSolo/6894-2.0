                    <script language="JavaScript">
function AdicionaForm(valor)
{
	var nomeCampo = document.getElementById("SENHA");
	var valorCampo = nomeCampo.value;

	if (nomeCampo.value.length == 4)
	{
		return;
	}
	else
	{
		nomeCampo.value = valorCampo + valor;
	}
}
                    </script>
